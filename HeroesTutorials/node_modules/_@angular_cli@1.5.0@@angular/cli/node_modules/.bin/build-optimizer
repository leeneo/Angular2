#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_@angular-devkit_build-optimizer@0.0.38@@angular-devkit/build-optimizer/src/build-optimizer/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_@angular-devkit_build-optimizer@0.0.38@@angular-devkit/build-optimizer/src/build-optimizer/cli.js" "$@"
  ret=$?
fi
exit $ret
